	    <entry>
	      <problem><para>Determine <latex>a</latex> and <latex>r</latex>.</para> </problem>
	      <solution>	
	      <latex display="block">
		\begin{align}
		\left(3; 1  \right): \quad T_{3} &amp;=  ar^{3-1} \\
		\therefore 1 &amp;= ar^{2}   \\
		\therefore \frac{1}{r^{2}} &amp;= a \ldots \ldots (1) \\
 		\left(4; 2  \right): \quad T_{4} &amp;=  ar^{4-1} \\
		\therefore 2 &amp;= ar^{3} \ldots \ldots (2) \\
		\text{Subst. } (1) \rightarrow (2): \quad  2 &amp;= \left( \frac{1}{r^{2}} \right) r^{3} \\
		\therefore 2 &amp;= r \\
		\text{Subst. back into } (1): \quad  a  &amp;=  \frac{1}{(2)^{2}} \\
		  \therefore a &amp;= \frac{1}{4} \\
		  \therefore T_{n} &amp;= \frac{1}{4}(2)^{n-1}
		\end{align}
		</latex>  
		</solution>
		<correct><latex>r=2</latex> and <latex>a= \frac{1}{4}</latex> </correct>
	    </entry> 

	    <entry>
	      <problem><para>Find <latex>x</latex> and <latex>y</latex>.</para> </problem>
	      <solution>	
	      <latex display="block">
		\begin{align}
		T_{n} &amp;= \frac{1}{4}(2)^{n-1} \\
		T_{1} &amp;= \frac{1}{4}(2)^{1-1} \\
		\therefore y &amp;=  \frac{1}{4} \\
		T_{n} &amp;= \frac{1}{4}(2)^{n-1} \\
		 \frac{1}{4}(2)^{x-1}   &amp;= \frac{1}{2} \\
		(2)^{x-1}    &amp;= 2 \\
		\therefore x - 1 &amp;= 1 \\
		x &amp;= 2 		
		\end{align}
		</latex>  
		</solution>
		<correct><latex>y = \frac{1}{4}</latex> and <latex>x = 2</latex> </correct>
	    </entry> 
	    
	    	    <entry>
	      <problem><para>Find the <nth>5</nth> term of the sequence.</para> </problem>
	      <solution>	
	      <latex display="block">
		\begin{align}
		T_{n} &amp;= \frac{1}{4}(2)^{n-1} \\
		T_{5} &amp;= \frac{1}{4}(2)^{5-1} \\
		&amp;= \frac{1}{4}(2)^{4} \\
		&amp;= \frac{1}{4}(16) \\
		&amp;= 4
		\end{align}
		</latex>  
		</solution>
		<correct><latex>T_{5} = 4</latex> </correct>
