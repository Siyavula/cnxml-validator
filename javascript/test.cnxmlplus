	\begin{align}
	{S}_{n} &amp;= \frac{n}{2} \left(7n+15\right)  \\
	{S}_{1} &amp;= {T}_{1} = a \\
	{S}_{1} &amp;= \frac{n}{2} \left(7n+15\right)  \\
	&amp;= \frac{1}{2} \left(7(1)+15\right)  \\
	\therefore a &amp;=  11 \\
	{S}_{2} &amp;= \frac{2}{2} \left(7(2)+15\right)  \\
	  &amp;=   29 \\
