	&amp;=  18 - 11  \\
	&amp;=  7 \\
	\therefore T_{n} &amp;= a + (n-1)d \\ 
	&amp;= 11 + (n-1)(7) \\ 
	&amp;= 11 + 7n - 7  \\ 
	&amp;= 7n + 4 \\ 
	\therefore T_{6} &amp;= 7(6) + 4 \\ 
	&amp;= 46
	\end{align}
