	      <row>
		<entry><latex>T_{1}</latex></entry>
		<entry><latex>\frac{1}{2} </latex> </entry>
		<entry><latex>\frac{1}{2} </latex></entry>
		<entry><latex>\frac{1}{2} </latex></entry>
	      </row>
	      	<row>
		<entry><latex>T_{1} + T_{2}</latex></entry>
		<entry><latex>\frac{1}{2} + \frac{1}{4} </latex> </entry>
		<entry><latex>\frac{3}{4} </latex></entry>
		<entry><latex>\frac{1}{4} </latex></entry>
	      </row>
