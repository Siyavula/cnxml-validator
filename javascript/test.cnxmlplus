	    <header>
	      <para>Given the sequence <latex>7; <number>5.5</number>; 4; <number>2.5</number>;  \ldots </latex> </para>

	    </header>

	    <entry>
	      <problem>Find the next term in the sequence. </problem>
	      <solution>
		<latex display="block">
		\begin{align}
		d &amp;= <number>5.5</number> - 7  \\
		&amp;= <number>-1.5</number> \\
		\therefore T_{5} &amp; = <number>2.5</number> + (<number>-1.5</number>) \\
		&amp;= 1
		\end{align}
		</latex> 
		</solution>
	      <correct>1</correct>
	    </entry>

	    <entry>
	      <problem>Determine the general term of the sequence. </problem>
	      <solution>
		<latex display="block">
		\begin{align}
		T_{n} &amp;= a + (n-1)d   \\
		&amp;= 7 + (n-1)(<number>-1.5</number>) \\
		&amp;= <number>8.5</number> - <number>1.5</number>n 
		\end{align}
		</latex> 
		</solution>
		<correct><latex>T_{n} = <number>8.5</number> - <number>1.5</number>n  </latex></correct>
	    </entry> 
	    
	    <entry>
	      <problem>Which term has a value of <number>-23</number>?</problem>
	      <solution>
		<latex display="block">
		\begin{align}
		T_{n} &amp;= <number>8.5</number> - <number>1.5</number>n \\
		\therefore -23 &amp;=  <number>8.5</number> - <number>1.5</number>n \\
		<number>31.5</number> &amp;= <number>1.5</number>n \\
		\therefore n &amp;= 21 \\
		\text{Therefore } T_{21} &amp;= -23
		\end{align}
		</latex> 
		</solution>
		<correct><latex>T_{21} = -23 </latex></correct>
	    </entry> 
 
